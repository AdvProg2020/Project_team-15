<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="578.0" prefWidth="400.0"
            style="-fx-background-color: #eeeeee;" xmlns="http://javafx.com/javafx/8.0.251"
            fx:controller="Client.ViewController.principal.CreateNewDiscountController">
    <children>
        <Label alignment="CENTER" layoutY="55.0" prefHeight="27.0" prefWidth="400.0" text="Create New Discount:">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Label layoutX="85.0" layoutY="266.0" prefHeight="30.0" text="Percent:" textAlignment="CENTER" wrapText="true">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Label>
        <TextField fx:id="percentField" alignment="CENTER" layoutX="171.0" layoutY="266.0" prefHeight="30.0"
                   prefWidth="155.0"/>
        <Button layoutX="40.0" layoutY="445.0" mnemonicParsing="false" onAction="#createDiscount" prefHeight="47.0"
                prefWidth="319.0" text="Create Discount">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Label alignment="CENTER" layoutX="55.0" layoutY="133.0" prefHeight="27.0" text="Customer Name:"
               textAlignment="CENTER" wrapText="true">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Label>
        <TextField fx:id="customerField" alignment="CENTER" layoutX="173.0" layoutY="131.0" prefHeight="30.0"
                   prefWidth="155.0"/>
        <Button layoutX="40.0" layoutY="171.0" mnemonicParsing="false" onAction="#addCustomer" prefHeight="47.0"
                prefWidth="319.0" text="Add Customer">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Label layoutX="66.0" layoutY="301.0" prefHeight="30.0" text="Max Amount:" textAlignment="CENTER"
               wrapText="true">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Label>
        <TextField fx:id="maxAmountField" alignment="CENTER" layoutX="171.0" layoutY="301.0" prefHeight="30.0"
                   prefWidth="155.0"/>
        <Label layoutX="76.0" layoutY="336.0" prefHeight="30.0" text="Use Count:" textAlignment="CENTER"
               wrapText="true">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Label>
        <TextField fx:id="useCountField" alignment="CENTER" layoutX="171.0" layoutY="336.0" prefHeight="30.0"
                   prefWidth="155.0"/>
        <Label layoutX="59.0" layoutY="371.0" prefHeight="30.0" text="Beginning Date:" textAlignment="CENTER"
               wrapText="true">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Label>
        <TextField fx:id="beginningDateField" alignment="CENTER" layoutX="171.0" layoutY="371.0" prefHeight="30.0"
                   prefWidth="155.0"/>
        <Label layoutX="70.0" layoutY="406.0" prefHeight="30.0" text="Ending Date:" textAlignment="CENTER"
               wrapText="true">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Label>
        <TextField fx:id="endingDateField" alignment="CENTER" layoutX="171.0" layoutY="406.0" prefHeight="30.0"
                   prefWidth="155.0"/>
    </children>
</AnchorPane>
